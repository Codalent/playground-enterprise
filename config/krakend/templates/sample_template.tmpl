{
   "endpoint": "/admin/users",
  {{  include "headers_to_pass.tmpl"}},
   "backend": [
   {
     "url_pattern": "/users",
     "extra_config": {
       {{ include "rate_limit_backend.tmpl"}}
     },
     "host": ["{{ .env.host_case }}"]
   }
   ],
   "extra_config": {
     "github.com/devopsfaith/krakend-jose/validator": {
       "alg": "RS256",
       "jwk-url": "{{ .env.security }}"
     }
   }
 },
 {
   "endpoint": "/admin/devices",
   "output_encoding": "json",
   "querystring_params":["*"],
 {{  include "headers_to_pass.tmpl"}},
   "backend": [
   {
     "url_pattern": "/devices",
     "encoding": "json",
     "extra_config": {
       {{ include "rate_limit_backend.tmpl"}}
     },
     "host": ["{{ .env.host_case }}"]
   }
   ],
   "extra_config": {
     "github.com/devopsfaith/krakend-jose/validator": {
       "alg": "RS256",
       "jwk-url": "{{ .env.security }}"
     }
   }
 }