{
  "github_com/devopsfaith/krakend/transport/http/server/handler": {
      "name": [
        "krakend-static-live"
      ],
      "krakend-static-live": {
        "prefix": "/demo/",
        "path": "/etc/krakend/demo/"
      }
    },
    "github_com/devopsfaith/krakend-swagger": {
      "fraction": 1,
      "version": "1",
      "contact-name": "John Doe",
