{
  "host": [
    "{{.servers.grpc_trains}}"
  ],
  "group": "trains_classes",
  "url_pattern": "/train_finder.Trains/GetTrainClasses",
  "extra_config": {
    "backend/grpc": {
      "response_naming_convention": "camelCase",
      "output_remove_unset_values": true,
      "client_tls": {
        "allow_insecure_connections": true
      }
    }
  }
}
