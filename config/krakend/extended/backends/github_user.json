{
  "host": [
    "https://api.github.com"
  ],
  "url_pattern": "/users/{user}",
  "allow": [
    "avatar_url",
    "name",
    "company",
    "blog",
    "location",
    "mail",
    "hireable",
    "followers",
    "public_repos",
    "public_gists"
  ],
  "mapping": {
    "blog": "website"
  },
  "group": "user"
}
