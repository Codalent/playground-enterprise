"extra_config": {
    {{ $cnt := 0 }}
    {{ range $k, $v := . }}
        {{ if ne $cnt 0 }},{{end -}} 
        {{ if eq $k "doc" }}
            "documentation/openapi": {{ $v | marshal }}
        {{ else }}
            "{{$v.key}}": {{ $v.val | marshal }}
        {{ end }}
        {{ $cnt = 1 }}
    {{ end }}
}
